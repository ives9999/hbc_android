<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="bottom_text">
        <item name="android:textColor">@color/MY_BLACK</item>
        <item name="android:textSize">13dp</item>
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="member_home_text">
        <item name="android:textSize">16dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="edit_title">
        <item name="android:textSize">16dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="button_submit">
        <item name="android:textSize">18dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ShapeAppearance.Capsule" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="FabStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">end|bottom</item>
        <item name="srcCompat">@drawable/outline_cloud_upload_24</item>
        <item name="backgroundTint">@color/colorPrimary</item>
        <item name="tint">@color/white</item>
        <item name="useCompatPadding">true</item>
    </style>
</resources>